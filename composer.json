{
  "name": "8fold/php-shoop-extras",
  "description": "An extension of Shoop for data types that are not base data types.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Josh Bruce",
      "email": "josh@8fold.pro"
    }
  ],
  "require": {
    "php": "~7",
    "8fold/php-shoop": "dev-master@dev",
    "spatie/yaml-front-matter": "~2.0",
    "league/commonmark": "~1.5",
    "cache/filesystem-adapter": "^1.0",
    "knplabs/github-api": "^2.15",
    "php-http/guzzle6-adapter": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.1",
    "vlucas/phpdotenv": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Eightfold\\ShoopExtras\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eightfold\\ShoopExtras\\Tests\\": "./tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true
}
