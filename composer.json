{
  "name": "8fold/php-shoop-extras",
  "description": "An extension of Shoop for data types that are not base data types.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Josh Bruce",
      "email": "josh@8fold.pro"
    }
  ],
  "require": {
    "php": "7.*",
    "8fold/php-shoop": "0.4.*",
    "spatie/yaml-front-matter": "2.0.*",
    "league/commonmark": "1.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.1.*",
    "phpunit/php-code-coverage": "8.0.*"
  },
  "autoload": {
    "psr-4": {
      "Eightfold\\ShoopExtras\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eightfold\\ShoopExtras\\Tests\\": "./tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "prefer-stable": true
}
