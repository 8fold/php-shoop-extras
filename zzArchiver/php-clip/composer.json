{
  "name": "8fold/php-clip",
  "description": "Command Line Interface Processor",
  "type": "library",
  "version": "0.0.1",
  "license": "MIT",
  "authors": [{
      "name": "Josh Bruce",
      "email": "josh@joshuabruce.com"
  }],
  "require": {
    "php": ">=7.0.0",
    "vlucas/phpdotenv": "^3.3",
    "guzzlehttp/guzzle": "^6.3",
    "8fold/php-shoop": "^0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.0"
  },
	"repositories": [
	    {
	        "type": "path",
	        "url": "../php-shoop",
	        "name": "8fold/php-shoop"
	    }
	],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Eightfold\\Clip\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eightfold\\Clip\\Tests\\": "tests/"
    }
  }
}
