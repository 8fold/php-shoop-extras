{
    "name": "8fold/php-git-simple",
    "description": "Handling Git.",
    "version": "0.0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Bruce",
            "email": "josh@8fold.pro"
        }
    ],
    "require": {
        "php": ">=7.0",
        "8fold/process": "^0.0.1"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "7.5.6",
        "phpunit/php-code-coverage": "^6.0.7"
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "path",
            "url": "../UtilProcess"
        }
    ],
    "autoload": {
        "psr-4": {
            "Eightfold\\GitSimple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eightfold\\GitSimple\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
